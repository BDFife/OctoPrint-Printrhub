<div class="browser column">

  {% set printrbar_title = 'PROJECTS LIBRARY' %}
  {% include 'html/printrbar.jinja2' %}
  <div class="searchbar">
    <input type="text" class="form-control search-field" placeholder="search">
  </div>

  <div id="content">
    <p> This is not Printrhub. It doesn't really look like it either</p>
    <p> Here are the files loaded on this system: </p>
    <ul>
      {% for file in files['local'] %}
      <li> <p>{{file}}</p><img src="/plugin/Printrhub/static/img/{{file}}.png" height="248" width="253"</img> </li>
      {% endfor %}
    </ul>
  </div> <!-- content -->

  <div id="form">
    <form action="plugin/Printrhub/upload?apikey=" method="post" enctype="multipart/form-data">
      <div>
        <label for="file">Choose file to upload</label>
        <input type="file" id="file" name="file"/>
      </div>
      <div>
        <button>Submit</button>
      </div>
    </form>
  </div> <!-- form -->

</div> <!-- browser -->
